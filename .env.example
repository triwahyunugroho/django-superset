# PostgreSQL Configuration
POSTGRES_DB=superset
POSTGRES_USER=superset
POSTGRES_PASSWORD=change_this_password

# Superset Configuration
SUPERSET_SECRET_KEY=change_this_secret_key
SUPERSET_ADMIN_USER=admin
SUPERSET_ADMIN_PASSWORD=admin
SUPERSET_ADMIN_EMAIL=admin@example.com
GUEST_TOKEN_JWT_SECRET=change_this_guest_token_secret

# Superset Service Account (for Django integration)
SUPERSET_SERVICE_USER=django_service
SUPERSET_SERVICE_PASSWORD=change_this_service_password

# Django Configuration
DEBUG=True
DJANGO_SECRET_KEY=change_this_django_secret
ALLOWED_HOSTS=localhost,127.0.0.1,django,caddy

# URLs
SUPERSET_URL=http://superset:8088
